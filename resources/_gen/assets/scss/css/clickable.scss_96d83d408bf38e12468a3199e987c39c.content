.clickable{display:inline-flex;justify-content:center;align-items:center;max-width:100%;padding:10px;color:inherit;border:none;font-family:poppins,sans-serif;font-size:1em;text-decoration:none;min-width:50px;min-height:50px;margin:10px;padding:10px 15px;background:#eee;color:#000}.clickable:hover{background:#b3e5fc}.clickable .clickable_text{text-align:center;font-family:poppins,sans-serif;font-weight:500}.clickable[data-icon]:not([data-text]){padding:0}.clickable[data-icon]:not([data-text]) .clickable_icon{font-size:1.2em}.clickable[data-text]{padding:10px 15px}.clickable[data-icon][data-text] .clickable_icon{margin-right:.8em}.clickable[data-style=rounded]{min-width:50px;min-height:50px;margin:5px;padding:10px 15px;border-radius:999px;background:#424242;color:#fff}.clickable[data-style=rounded]:hover{background:#03a9f4}.clickable[data-active=false]{background:0 0}.clickable[data-active=false]:hover{background:#eee}.clickable[data-active=true]{background:#b3e5fc}